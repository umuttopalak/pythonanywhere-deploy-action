{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch with ts-node",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "ts-node"
            ],
            "args": [
                "${workspaceFolder}/src/index.ts"
            ],
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/src/**/*.ts"
            ]
        }
    ]
}
